<Window x:Class="GubaidullinAutoService.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="GubaidullinAutoService" FontFamily="Tw Cen MT" Height="450" Width="800" Icon="/Resources/service_logo.ico">
    <!--DESKTOP-OD5ORN5\SQLEXPRESS-->
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
        </Grid.RowDefinitions>
        <Image HorizontalAlignment="Left" Source="/Resources/service_logo.png"/>
        <TextBlock VerticalAlignment="Center" FontSize="30" HorizontalAlignment="Center" Text="Услуги автосервиса"></TextBlock>
        <Frame NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered" x:Name="MainFrame" Grid.Row="1"></Frame>
        <Button Content="Back" Name="BackButton" Width="175" Height="30" HorizontalAlignment="Right" Margin="5" Click="BackButton_Click"></Button>
    </Grid>
</Window>
